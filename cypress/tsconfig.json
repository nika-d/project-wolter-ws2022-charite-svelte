{
	"compilerOptions": {
		"baseUrl": "..",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"]
		},
		"target": "es5",
		"lib": ["es5", "dom"],
		"types": ["cypress", "node", "cypress-real-events"],
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"isolatedModules": false
	},
	"include": ["**/*.ts"]
}
