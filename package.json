{
	"bezeichnungLang": "llp-frontend",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build --mode production",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"css-unused-selector:ignore\"",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"test": "vitest run --segfault-retry=3",
		"coverage": "vitest run --coverage"
	},
	"dependencies": {
		"@types/bootstrap": "^5.2",
		"axios": "^1.1",
		"bootstrap": "^5.2",
		"cypress-real-events": "^1.7.1",
		"fuzzysort": "^2.0.1",
		"popper.js": "^1.16",
		"vite": "^3.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "latest",
		"@sveltejs/adapter-node": "latest",
		"@sveltejs/kit": "latest",
		"@testing-library/svelte": "^3.1",
		"@testing-library/user-event": "^14.4",
		"@tsconfig/svelte": "^3.0",
		"@types/jest": "^29.2",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.33",
		"@typescript-eslint/parser": "^5.33",
		"cypress": "10.11",
		"dotenv": "^16.0",
		"eslint": "^8.22",
		"eslint-config-prettier": "^8.5",
		"eslint-plugin-cypress": "^2.12",
		"eslint-plugin-svelte3": "^4.0",
		"flush-promises": "^1.0",
		"isomorphic-fetch": "^3.0",
		"jsdom": "^20.0",
		"prettier": "^2.7",
		"prettier-plugin-svelte": "^2.7",
		"purgecss-from-svelte": "^2.0",
		"sass": "^1.55.0",
		"simple-svelte-autocomplete": "^2.4",
		"source-map-support": "^0.5",
		"svelte": "^3.49",
		"svelte-check": "^2.8",
		"svelte-preprocess": "^4.10",
		"ts-node": "^10.9",
		"tslib": "^2.4",
		"typescript": "^4.6",
		"uuid": "^9.0",
		"vitest": "^0.24"
	},
	"babel": {
		"plugins": [
			"@babel/plugin-proposal-class-properties"
		],
		"presets": [
			[
				"@babel/preset-env"
			]
		]
	},
	"browserslist": [
		"since 2017-06"
	]
}
